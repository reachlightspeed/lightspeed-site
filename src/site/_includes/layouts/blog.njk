          </svg>
        </nav>
      </header>

      <section class="box z-10 relative">
        <p>{{ page.date | readableDate }}</p>
        <h1>{{ title | safe }}</h1>
        {{ content | safe }}
      </section>

      <section class="spacepurple-section"></section>

    </main>

  </body>

</html>